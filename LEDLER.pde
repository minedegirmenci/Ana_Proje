void setup() {
// initialize the digital pin as an output.
// Pin 13 has an LED connected on most Arduino boards:
pinMode(23, OUTPUT);
pinMode(25, OUTPUT);
pinMode(27, OUTPUT);
pinMode(29, OUTPUT);
pinMode(31, OUTPUT);
pinMode(33, OUTPUT);
pinMode(35, OUTPUT);
pinMode(37, OUTPUT);
}
 
void loop() {
digitalWrite(23, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(23, LOW);    // set the LED off
delay(30);              // wait for a second
 
digitalWrite(25, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(25, LOW);    // set the LED off
delay(30);              // wait for a second
 
digitalWrite(27, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(27, LOW);    // set the LED off
delay(30);              // wait for a second
 
digitalWrite(29, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(29, LOW);    // set the LED off
delay(30);              // wait for a second
 
digitalWrite(31, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(31, LOW);    // set the LED off
delay(30);              // wait for a second
 
digitalWrite(33, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(33, LOW);    // set the LED off
delay(30);              // wait for a second
 
digitalWrite(35, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(35, LOW);    // set the LED off
delay(30);              // wait for a second
 
digitalWrite(37, HIGH);   // set the LED on
delay(30);              // wait for a second
digitalWrite(37, LOW);    // set the LED off
delay(30);              // wait for a second
}
